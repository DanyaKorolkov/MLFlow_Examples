name: ML_Experiment

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      train_size: {type: float, default: 0.8}
      penalty: {type: str, default: "l2"}
      C: {type: float, default: 2}
      fit_intercept: {type: bool, default: False}
    command: |
        python train.py \
            --train_size {train_size} \
            --penalty {penalty} \
            --C {C} \
            --fit_intercept {fit_intercept} \